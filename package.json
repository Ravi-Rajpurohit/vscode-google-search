{
	"name": "google-search",
	"displayName": "Google Search",
	"description": "Google search anything from vscode editor, output view and terminal.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.50.0"
	},
	"icon": "images/icon.png",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:google-search.makeSearch",
		"onCommand:google-search.makeSearchFromTerminal"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "google-search.makeSearch",
				"title": "Google selected text "
			}
		],
		"keybindings": [
			{
				"command": "google-search.makeSearch",
				"key": "ctrl+alt+g",
				"mac": "cmd+alt+g",
				"when": "editorHasSelection"
			},
			{
				"command": "google-search.makeSearchFromTerminal",
				"key": "ctrl+alt+k g",
				"mac": "cmd+alt+k g",
				"when": "terminalFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "google-search.makeSearch",
					"when": "editorHasSelection",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"publisher": "Ravi-Rajpurohit",
	"repository": {
		"type": "git",
		"url": "https://github.com/Ravi-Rajpurohit/vscode-google-search"
	}
}
